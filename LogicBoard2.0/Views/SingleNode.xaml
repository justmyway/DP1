<UserControl x:Class="LogicBoard2._0.Views.SingleNode"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="200">
    <Grid>
        <Ellipse x:Name="InputProbe" Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="50" Stroke="Black" VerticalAlignment="Center" Width="50"/>
        <Ellipse x:Name="OutputProbe" Fill="#FFF4F4F5" HorizontalAlignment="Right" Height="50" Stroke="Black" VerticalAlignment="Center" Width="50"/>
        <Rectangle Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="80" Margin="50,10,0,0" Stroke="Black" VerticalAlignment="Top" Width="100"/>
        <TextBlock x:Name="NodeName" HorizontalAlignment="Center" TextWrapping="Wrap" Text="Not" VerticalAlignment="Center" FontWeight="Bold" FontSize="40" OpacityMask="Black" Background="#66FFFFFF"/>
    </Grid>
</UserControl>
